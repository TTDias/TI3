.botao:hover {
    background-color: rgb(255, 255, 255);
}

.botao {
    width: auto;
    height: auto;
    padding-top: 20px;
    padding-right: 20px;
    padding-bottom: 20px;
    padding-left: 20px;
    margin-bottom: 20px;
    -unity-text-outline-color: rgba(92, 122, 178, 0);
    -unity-background-image-tint-color: rgb(131, 204, 245);
    background-color: rgb(133, 180, 200);
    font-size: 500%;
    -unity-text-align: middle-center;
    min-width: auto;
    min-height: auto;
    position: relative;
}
